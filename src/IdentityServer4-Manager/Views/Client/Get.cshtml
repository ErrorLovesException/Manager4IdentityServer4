@{
}

@model IdentityServer4.EntityFramework.Entities.Client
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="modalTitle">
        Create Claims
    </h4>
</div>
<table class="table table-bordered">
    <tr>
        <td>ID</td>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <td>Client Name</td>
        <td>@Model.ClientName</td>
    </tr>
    <tr>
        <td>Client Uri</td>
        <td>@Model.ClientUri</td>
    </tr>
</table>
<div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
    <button id="btn_create_client" type="button" class="btn btn-primary btn-loading"
            data-loading-text="<i class='fa fa-spinner fa-spin '></i>&nbsp; Create">
        Create
    </button>
</div>